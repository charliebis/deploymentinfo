{
    "name": "bisutil/deploymentinfo",
    "description": "Reads deployment info JSON file and exposes them as config values",
    "type": "library",
    "authors": [
        {
            "name": "Charles Edwards",
            "email": "charlie@charliee.co.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2|^8.1",
        "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*|8.*|^9.0|^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "illuminate/support": "^8.5|^9.0|^10.0",
        "laravel/laravel": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bisutil\\DeploymentInfo\\": "src/",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bisutil\\DeploymentInfo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bisutil\\Deploymentinfo\\DeploymentinfoServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
